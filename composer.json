{
    "name": "jwcobb/ticketevolution-php-dataloaders",
    "type": "project",
    "description": "Some simple scripts for caching data from the Ticket Evolution API.",
    "keywords": ["Ticket Evolution", "ticket"],
    "homepage": "https://github.com/jwcobb/ticketevolution-php-dataloaders",
    "license": "BSD-3-Clause",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ticketevolution/ticketevolution-php",
            "branch": "2.0"
        }

    ],
    "authors": [
        {
            "name": "J Cobb"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "ticketevolution/ticketevolution-php": "~2.0"
    },
    "autoload": {
        "psr-0": {
            "Zend": "vendor/",
            "TicketEvolution": "vendor/",
            "DataLoader": "library/",
            "": "library/"
        }
    },
    "minimum-stability": "dev",
    "support": {
        "issues": "https://github.com/jwcobb/ticketevolution-php-dataloaders/issues",
        "wiki": "https://github.com/jwcobb/ticketevolution-php-dataloaders/wiki",
        "source": "https://github.com/jwcobb/ticketevolution-php-dataloaders"
    }
}
